<div class="container">
  <h2>New article</h2>

  <%= form_with model: @task do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :content %>
    <%= f.text_field :content %>
    <%= f.submit%>
    <% end %>

  <%= link_to 'Back to list', articles_path, :class => "btn btn-primary" %>
</div>
